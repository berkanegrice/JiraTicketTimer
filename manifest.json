{
    "manifest_version": 3,
    "name": "JiraExtension",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [        
        "cookies"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "clickChecker.js"
            ]
        }
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}